    "@react-navigation/native-stack": "^6.9.20",
    "axios": "^1.6.7",
    "expo": "~50.0.7",
    "expo-checkbox": "~2.7.0",
    "expo-status-bar": "~1.11.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
